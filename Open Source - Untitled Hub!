local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("Duplicate HUB { version: 3 }", "BloodTheme")
local Tab = Window:NewTab("Home")
local player = game.Players.LocalPlayer
local displayName = player.DisplayName

local Section = Tab:NewSection("Display Name: " .. displayName)
local username = player.Name

Section = Tab:NewSection("Username: " .. username)
local accountAge = player.AccountAge

Section = Tab:NewSection("Account Age: " .. accountAge .. " days")
local gameName = game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name
local executor = "???"
if identifyexecutor then
    executor = identifyexecutor()
end
Section = Tab:NewSection("Executor: " .. executor)

Section = Tab:NewSection("Game: " .. gameName)
Tab = Window:NewTab("Main")
Section = Tab:NewSection("List of Hub that i showcase in my YT")
Section = Tab:NewSection("Press the 3 Dot on the right. To see a Description!")

Section:NewButton("SystemBroken", "My first showcase in YT", function()
    local credits = "SystemBroken made by system_calix"
    loadstring(game:HttpGet("https://raw.githubusercontent.com/H20CalibreYT/SystemBroken/main/script"))()
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "Notification";
        Text = "SystemBroken script loaded!";
        Duration = 5;
    })
end)

